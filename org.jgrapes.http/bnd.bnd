api_version = 1.0.0.${tstamp}

Import-Package: org.jdrupes.httpcodec.*;version='[2.0.3,3)', *

-sources: true
-runee: JavaSE-1.8
Export-Package: \
	org.jgrapes.http,\
	org.jgrapes.http.annotation,\
	org.jgrapes.http.events
	
